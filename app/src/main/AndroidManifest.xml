<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unii.entertainment.teammaker">

    <application
        android:name=".TeamMakerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CategoryListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".player.view.PlayerActivity" />
        <activity
            android:name=".wizzard.viewmodel.WizardActivity"
            android:label="@string/title_activity_wizard"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>